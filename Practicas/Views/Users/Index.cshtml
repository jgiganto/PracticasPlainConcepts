<h2>Users</h2>


<table class="table table-bordered">
    <thead>

    <td>Nombre</td>
    <td>Apellido</td>
    <td>Pais</td>
    <td>Dirección</td>
    </thead>

    @foreach (var user in Model.Users)
    {
    <tr>
        <td>@user.Name</td>
        <td>@user.Lastname</td>
        <td>@user.Country</td>
        <td>@user.Address</td>
    </tr>
    }
</table>
<form method="post" id="formulario">
    <div>
        <h1>Login</h1>
        <label>Usuario &nbsp;</label>
        <input type="text" name="camponombre" id="camponombre" placeholder="Introduzca el nombre" required /><br />
        <label>Password</label>
        <input type="password" name="campopw" id="campopw" placeholder="Contraseña" required /><br />
